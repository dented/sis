    scss:
      body {
        font-family: 'Roboto', sans-serif;
      }
      .main-background {
        background-image: url('https://whub-assets.s3.amazonaws.com/uploads/03d/c17/138/728/b9d/113/6de/da2/be4/449/large_d82e002afffe6e8a4dd58c51897ca3ff.jpg');
        background-repeat: no-repeat;
        background-size: cover;
        -webkit-background-size: cover;
        background-position: center center;
        height: 100vh;
        .mask {
          width: 100%;
          height: 100%;
        }
      }

      .site-navbar {
        transition: all .3s;
        padding: 0;
        .navbar-nav {
          li {
            min-width: 120px;
            a {
              color: #fff;
            }
          }
        }
        a {
          color: #188bef;
          font-weight: bold;
          padding: 25px 0;
        }
        .active, .active a, li a.current:hover, li a:hover {
          color: darken(#fff, 10%);
          background: 0 0;
        }
        .navbar-toggle .icon-bar {
          background: #fff;
        }
        .navbar-toggle {
          border: 1px solid #fff;
          border-radius: 0;
        }
        .navbar-toggler-icon i {
          font-size: 1.5rem;
          font-weight: 100;
          padding-top: 1px;
        }
        .navbar-toggler {
          color: #fff;
          padding: .25rem .5rem;
        }
        .navbar-collapse {
          .nav-item {
            text-align: center;
          }
        }

        &.sticky {
          background-color: rgba(24,139,239,0.5);
          .navbar-nav {
            background-color: inherit;
            a {
              color: #fff;
              &:hover {
                color: darken(#fff, 10%);
              }
            }
          }
          z-index: 100000;
          // padding: 12px 0;
          box-shadow: 0 0 15px rgba(0,0,0,.1);
        }
      }

      .bg-dark {
        &.extra-dark {
          background-color: #000 !important;
        }
      }
      #map {
        iframe {
          border: none;
          position: relative;
          margin-bottom: -2px;
        }
        .map-overlay{
          transition: all 0.6s ease;
          background-color: black;
          opacity: 0;
          position: absolute;
          top: 0;
          bottom: 0;
          width:100%;
          color: white;
          display:flex;
          align-items: center;
          justify-content: center;
          &:hover{
            opacity: 0.5;
            cursor: pointer;
          }
        }
      }
      .bg-blue {
        background-color: #33447a;
        &.extra-blue {
          background-color: #33c5f3;
        }
      }
      .supporting-cards {
        .card {
          // min-height: 135px;
          border: none;
          img {
            border: none;
          }
        }
      }
      .btn-white {
        background-color: #fff;
        color: #188bef;
        border-color: #fff;
        &:hover {
          background-color: #188bef;
          color: #fff;
          border-color: #188bef;
        }
      }
      .text-blue {
        color: #188bef;
      }
      .text-underline {
        border-bottom: 1px solid #188bef;
      }
      .text-bold {
        font-weight: 700;
      }
      .w-break {
        word-wrap: break-word;
      }
      .nav-metrics {
        li {
          padding: 0 20px;
          strong {
            font-size: 2.2em;
            text-align: center;
          }
          span {
            font-size: 0.8em;
            word-wrap: break-word;
            color: #fff;
            text-align: center;
          }
        }
      }
      .text-red {
        color: firebrick;
      }
      .w-150 {
        width: 150px;
      }
      .w-250 {
        width: 250px;
      }

      @media only screen and (max-width: 768px) {
        .site-navbar {
          margin-bottom: 0;
          padding: 0;
          a.navbar-brand {
            font-size: 2.3em;
            img {
              padding: 5px;
              width: 80px;
              height: 45px;
            }
          }
          .container {
            padding-right: 15px;
            padding-left: 15px;
            border-radius: 0;
          }
          .navbar-toggle .icon-bar {
            background: #000;
          }
        }
        .main-background {
          text-align: center;
        }
        .nav-metrics {
          li {
            padding: 0 10px;
          }
        }
        .sis-about {
          text-align: center;
          img {
            margin-bottom: 10px;
          }
        }
        .mt-xs-3 {
          margin-top: (1em * 0.75);
          margin-left: 0 !important;
        }
        .d-xs-block, .d-sm-block {
          display: block;
        }
      }

      @media only screen and (max-width: 480px) {
        .site-navbar {
          margin-bottom: 0;
          padding: 0;
          a.navbar-brand {
            font-size: 1.8rem;
            img {
              width: 80px;
              height: 45px;
            }
          }
          .container {
            padding-right: 15px;
            padding-left: 15px;
            border-radius: 0;
          }
          .navbar-toggle .icon-bar {
            background: #000;
          }
        }
        .nav-metrics {
          li {
            padding: 0 10px;
          }
        }
        .main-background {
          text-align: center;
        }
        .sis-about {
          text-align: center;
          img {
            margin-bottom: 10px;
          }
        }
        .mt-xs-3 {
          margin-top: (3em * 0.75);
          margin-left: 0 !important;
        }
        .d-xs-block, .d-sm-block {
          display: block;
        }
        .tab-pane {
          display: block !important;
          padding: 10px 0;
          border-bottom: 1px solid #ccc;
        }
      }

      /* PILLARS */
      .pillars {
        width: 100%;
        overflow: hidden;
        color: white;
        position: relative
      }

      .pillars .pillars-list li {
        list-style: none;
      }
      .pillars .pillar-content {
        color: #000;
      }

      @media (min-width:992px) {
        .pillars ul {
          width: 110%
        }
      }

      .pillars ul li .pillars-nav {
        width: 100%;
        overflow: hidden;
        position: relative;
        background-color: rgba(0,0,0,0.6);
      }
      .pillars ul li .pillars-nav.active {
        background-color: rgba(0,0,0,0.9);
      }

      .pillars ul li .pillars-nav:hover {
        -o-transition: all 250ms ease-in-out;
        -webkit-transition: all 250ms ease-in-out;
        transition: all 250ms ease-in-out;
        cursor: pointer;
      }

      @media (min-width:992px) {
        .pillars ul li .pillars-nav {
          float: left;
          width: 16.6%;
          height: 350px;
          margin: 0 .5%;
          -webkit-transform-style: preserve-3d;
          -moz-transform-style: preserve-3d;
          -ms-transform-style: preserve-3d;
          transform-style: preserve-3d;
          -webkit-transform: skew(-20deg);
          -moz-transform: skew(-20deg);
          -o-transform: skew(-20deg);
          -ms-transform: skew(-20deg);
          transform: skew(-20deg);
        }
      }

      @media (min-width:992px) {
        .pillars ul li .pillars-nav > * {
          -webkit-transform-style: preserve-3d;
          -moz-transform-style: preserve-3d;
          transform-style: preserve-3d;
          -webkit-transform: skew(20deg);
          -moz-transform: skew(20deg);
          -o-transform: skew(20deg);
          -ms-transform: skew(20deg);
          transform: skew(20deg);
        }
      }
      @media (max-width:992px){
        .image{
          width:100vw;
          height:230px;

        }
      }
      @media (min-width:992px) {
        .pillars ul li .pillars-nav div.image {
          width: 170%;
          height: 170%;
          margin: 0 0 0 -35%;
        }
      }

      .pillars ul li .pillars-nav div.image {
        background-position: center center;
        background-size: cover;
        position: absolute;
        opacity: 1;
      }

      .pillars ul li .pillars-nav .title {
        display: block;
        background-color: rgba(0,0,0,0.2);
        padding: 10px 0;
        margin: 0 auto;
        text-align: center;
        text-transform: uppercase;
        color: #fff;
        position: absolute;
        top: 45%;
        left: -5%;
        width: 110%;
        font-weight: 100;
      }

      .pillars ul li .pillars-nav .title .text-white {
        font-weight: 600;
        display: inline;
      }

      @media (min-width:992px) {

        .pillars ul li .pillars-nav {
          &:hover {
            //opacity: .9;
            background-color: rgba(0,0,0,1);
          }
          div.image {
            opacity: .5;
            top: -25%;
            position: relative;
          }
        }

        .pillars ul li:first-child .pillars-nav,
        .pillars ul li:last-child .pillars-nav {
          width: 18.5%;
        }

        .pillars ul li:first-child .pillars-nav {
          margin-left: -10%;

          div.image {
            margin: 0;
          }

          .title {
            left: 10%;
          }
        }
        .pillars ul li:last-child .pillars-nav {
          .title {
            left: -25%;
          }
        }
      }

      @media (max-width:992px) {
        .pillars {
          display: none;
        }
      }

      /* END PILLARS */
      .fs-180 {
        font-size: 1.8em;
      }
      .card-img-top {
        max-width: 125px;
      }
      .get-it-now {
        width: 336px;
        height: 78px;
        font-family: TheHistoriaDemo;
        font-size: 73px;
        font-weight: normal;
        font-style: normal;
        font-stretch: normal;
        line-height: 1.1;
        letter-spacing: -1.9px;
        text-align: left;
        color: var(--bright-light-blue);
      }

